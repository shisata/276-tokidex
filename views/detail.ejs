<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" name="viewport" content="width=device-width, initial-scale=1">
    <title>Detail</title>
  </head>
  <body>
    <link rel="stylesheet" href="style.css">
    <div class="side-nav">
      <a href="https://tokidex.herokuapp.com/home">Tokidex</a>
      <a href="https://tokidex.herokuapp.com/create">Creation</a>
      <a href="https://tokidex.herokuapp.com/comparison">Comparison</a>
    </div>
    <div class="display-box">
      <h1 class="logo">Tokidex</h1>
      <ul>
        <% results.forEach(function(r) { %>
          <li class="tokimon-box">
            <table>
              <% var initial = r.name; %>
              <td><img src="" alt='<%= initial[0] %>'></td>
              <td>
                <p class="tokimon-index">#<%= r.id %></p>
                <h3 class="tokimon-name"><%= r.name %></h3>
                <ul class="element-list">
                  <li class='<%= r.element1 %>'><%= r.element1 %></li>
                  <li class='<%= r.element2 %>'><%= r.element2 %></li>
                  <li class='<%= r.element3 %>'><%= r.element3 %></li>
                </ul>
              </td>
            </table>
          </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>
